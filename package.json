{
  "name": "3d-points",
  "private": true,
  "description": "Need to create digital twin of a building? Get to know reality capture and scan-to-BIM technics like a laser scanning to create 3D BIM model. Free video tutorials",
  "version": "0.1.0",
  "author": "Lukasz W",
  "dependencies": {
    "14": "^3.1.6",
    "gatsby": "^2.29.3",
    "gatsby-plugin-anchor-links": "^1.1.1",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-google-gtag": "^2.8.0",
    "gatsby-plugin-google-tagmanager": "^2.11.0",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-transition-link": "^1.20.2",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-source-prismic": "^3.3.1",
    "gh-pages": "^3.1.0",
    "node": "^14.15.0",
    "node-sass": "^4.13.1",
    "prismic-reactjs": "^1.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-device-detect": "^1.15.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-player": "^1.15.3",
    "react-scrollable-anchor": "^0.6.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-root-import": "^2.0.5",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.14.2",
    "stylelint-webpack-plugin": "^1.2.3",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "develop-lan": "gatsby develop -H $HOSTNAME -p 8000",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby,.dev/unit-testing\" && exit 1",
    "lint": "tsc --noEmit && eslint '**/*.{js,jsx,ts,tsx}' .gitignore --quiet --fix",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:github": "npm run build && node ./scripts/deploy-github"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js, *.jsx, *.ts, *.tsx": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ]
  }
}
